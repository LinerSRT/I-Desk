<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VideoPlayerView">
        <attr name="toFullScreenIcon" format="reference" />
        <attr name="toBaseScreenIcon" format="reference" />
        <attr name="playIcon" format="reference" />
        <attr name="pauseIcon" format="reference" />
        <attr name="seekBarProgressDrawable" format="reference" />
        <attr name="seekBarThumb" format="reference" />
        <attr name="currentPositionColor" format="color|reference" />
        <attr name="durationColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="RecordView">
        <attr name="slide_to_cancel_text" format="string" />
        <attr name="slide_to_cancel_arrow" format="reference" />
        <attr name="slide_to_cancel_margin_right" format="dimension" />
        <attr name="slide_to_cancel_bounds" format="dimension" />
        <attr name="slide_to_cancel_arrow_color" format="color|reference" />
        <attr name="counter_time_color" format="color|reference" />

    </declare-styleable>

    <declare-styleable name="RecordButton">
        <attr name="mic_icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="CircleProgressBar">
        <attr name="progressMin" format="integer" />
        <attr name="progressMax" format="integer" />
        <attr name="progress" format="integer" />
        <attr name="progressStartAngle" format="integer" />
        <attr name="progressBarColor" format="color|reference" />
        <attr name="progressBarBackgroundColor" format="color|reference" />
        <attr name="progressBarThickness" format="dimension|reference" />
        <attr name="progressBarPadding" format="dimension|reference" />
        <attr name="progressBarIconPadding" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="YSTextView">
        <attr name="ys_type">
            <enum name="Bold" value="1" />
            <enum name="Light" value="2" />
            <enum name="Medium" value="3" />
            <enum name="Regular" value="4" />
            <enum name="RegularItalic" value="5" />
            <enum name="Thin" value="6" />
        </attr>
    </declare-styleable>

    <declare-styleable name="YSEditText">
        <attr name="yse_type">
            <enum name="Bold" value="1" />
            <enum name="Light" value="2" />
            <enum name="Medium" value="3" />
            <enum name="Regular" value="4" />
            <enum name="RegularItalic" value="5" />
            <enum name="Thin" value="6" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SwipeableRecyclerView">
        <attr name="left_bg_color" format="color|reference" />
        <attr name="left_image" format="integer|reference" />
        <attr name="left_text" format="string|reference" />
        <attr name="left_text_size" format="integer|reference" />
        <attr name="left_text_color" format="color|reference" />
        <attr name="right_bg_color" format="color|reference" />
        <attr name="right_image" format="integer|reference" />
        <attr name="right_text" format="string|reference" />
        <attr name="right_text_size" format="integer|reference" />
        <attr name="right_text_color" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="MediaPickerView">
        <attr name="picker_max_count" format="integer" />
        <attr name="picker_column" format="integer" />
        <attr name="picker_item_decoration" format="reference" />
        <attr name="picker_item_add_cover" format="reference" />
        <attr name="picker_item_delete_icon" format="reference" />
    </declare-styleable>


    <declare-styleable name="ExpandLayout">
        <attr name="el_duration" format="integer" />
        <attr name="el_expanded" format="boolean" />
        <attr name="el_parallax" format="float" />
        <attr name="android:orientation" />
    </declare-styleable>


    <declare-styleable name="GradientImageView">
        <attr name="giv_gradient_color" format="color|reference" />
        <attr name="giv_gradient_color_alpha" format="float|reference" />
        <attr name="giv_gradient_height" format="integer|reference" />
        <attr name="giv_image" format="integer|reference" />
    </declare-styleable>



    <declare-styleable name="VerticalStepperFormView">
        <attr name="form_next_button_text" format="string" />
        <attr name="form_last_button_text" format="string" />
        <attr name="form_cancel_button_text" format="string" />
        <attr name="form_confirmation_step_title_text" format="string" />
        <attr name="form_confirmation_step_subtitle_text" format="string" />
        <attr name="form_circle_size" format="dimension" />
        <attr name="form_circle_text_size" format="dimension" />
        <attr name="form_title_text_size" format="dimension" />
        <attr name="form_subtitle_text_size" format="dimension" />
        <attr name="form_error_message_text_size" format="dimension" />
        <attr name="form_vertical_line_width" format="dimension" />
        <attr name="form_horizontal_margin_from_step_numbers_to_content" format="dimension" />
        <attr name="form_disabled_elements_background_color" format="color" />
        <attr name="form_circle_background_color" format="color" />
        <attr name="form_next_button_background_color" format="color" />
        <attr name="form_next_button_pressed_background_color" format="color" />
        <attr name="form_cancel_button_background_color" format="color" />
        <attr name="form_cancel_button_pressed_background_color" format="color" />
        <attr name="form_bottom_navigation_background_color" format="color" />
        <attr name="form_circle_text_color" format="color" />
        <attr name="form_title_text_color" format="color" />
        <attr name="form_subtitle_text_color" format="color" />
        <attr name="form_next_button_text_color" format="color" />
        <attr name="form_next_button_pressed_text_color" format="color" />
        <attr name="form_cancel_button_text_color" format="color" />
        <attr name="form_cancel_button_pressed_text_color" format="color" />
        <attr name="form_error_message_text_color" format="color" />
        <attr name="form_display_bottom_navigation" format="boolean" />
        <attr name="form_display_step_buttons" format="boolean" />
        <attr name="form_display_cancel_button_in_last_step" format="boolean" />
        <attr name="form_display_step_data_in_subtitle_of_closed_steps" format="boolean" />
        <attr name="form_display_different_background_color_on_disabled_elements" format="boolean" />
        <attr name="form_include_confirmation_step" format="boolean" />
        <attr name="form_allow_non_linear_navigation" format="boolean" />
        <attr name="form_allow_step_opening_on_header_click" format="boolean" />
        <attr name="form_close_last_step_on_completion" format="boolean" />
        <attr name="form_alpha_of_disabled_elements" format="float" />
    </declare-styleable>
	
	    <declare-styleable name="SpaceNavigationView">
        <attr name="space_item_icon_size" format="dimension" />
        <attr name="space_item_icon_only_size" format="dimension" />
        <attr name="space_item_text_size" format="dimension" />
        <attr name="space_background_color" format="color" />
        <attr name="centre_button_color" format="color" />
        <attr name="active_item_color" format="color" />
        <attr name="inactive_item_color" format="color" />
        <attr name="centre_button_icon" format="reference" />
        <attr name="active_centre_button_icon_color" format="color" />
        <attr name="inactive_centre_button_icon_color" format="color" />
        <attr name="active_centre_button_background_color" format="color"/>
        <attr name="centre_part_linear" format="boolean" />
    </declare-styleable>
    <declare-styleable name="DisableLayout">
        <attr name="dl_enabled" format="boolean"/>
    </declare-styleable>

</resources>